<launch>
  <node pkg="dji_sdk" type="dji_sdk_node" name="dji_sdk">
    <!-- node parameters -->
    <param name="serial_name" type="string" value="/dev/ttyUSB0"/>
    <param name="baud_rate" type="int" value="230400"/>
    <param name="app_id" type="int" value="1027168"/>
    <param name="app_version" type="int" value="1"/>
    <param name="app_bundle_id" type="string" value="Black Widow"/>
    <param name="enc_key" type="string" value="c811bdf46d5412cf632e15ede58a9ece1df48eca77d74a1d72710c6706750111"/>
    <param name="groundstation_enable" type="int" value="1"/>
    <param name="uart_or_usb" type="int" value="0"/>
  </node>
  
  <node pkg="matrice_comm" type="matrice_comm_node" name="matrice_comm_node"/>

  <node pkg="guidance" type="GuidanceMultiCamNode" name="guidance_node">
    <param name="pub_left" type="bool" value="true"/>
    <param name="pub_right" type="bool" value="true"/>
    <param name="pub_depth" type="bool" value="false"/>
  </node>
</launch>
